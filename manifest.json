{
    "name": "Find Images",
    "description": "Quickly find images from selected text",
    "version": "1.2",
    "manifest_version": 2,
    "permissions": ["contextMenus", "tabs"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "icons": { 
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
